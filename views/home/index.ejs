<%# <!DOCTYPE html>%>
<html>
  <head>
    <meta charset='utf-8'>
    <title>Bartenda Home</title>
    <link rel='stylesheet' type='text/css' href='/../../css/header.css'>
    <link href="https://fonts.googleapis.com/css?family=Aclonica" rel='stylesheet'>
  </head>
  <body>
    <%# Created partial for header content%>
    <% include ../partials/header.ejs %>
    <main>
      <p><a href='/bartenda/new'>Add a drink to the bar</a></p>
      <div class='col1'></div>
      <div class='col2'>
        <% for (let post of allPosts) { %>
          <a href='/bartenda/<%= post._id %>'><img src='<%= post.url %>'></a>
          <h3><%= post.drinkName %></h3>
          <h4><%= post.submittedBy %></h4>
        <% } %>
      </div>
    </main>
    <footer>
    </footer>
  </body>
</html>
