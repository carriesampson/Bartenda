<%# <!DOCTYPE html>%>
<html>
  <%# Created partial for head content%>
  <% include ../partials/head.ejs %>
  <body>
    <%# Created partial for header content%>
    <% include ../partials/header.ejs %>
    <main>
      <div class='col1'></div>
      <div class='col2'>
        <% for (let post of allPosts) { %>
          <a href='/bartenda/<%= post._id %>'><img src='<%= post.img %>'></a>
          <h3><%= post.drinkName %></h3>
          <h4><%= post.submittedBy %></h4>
        <% } %>
      </div>
    </main>
    <%# Created partial for footer content%>
    <% include ../partials/footer.ejs %>
  </body>
</html>
